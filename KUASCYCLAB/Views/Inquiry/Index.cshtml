@model IEnumerable<KUASCYCLAB.Models.Inquiry>

@{
    ViewBag.Title = "詢價單";
}

<h2>詢價單</h2>

<p>
    @Html.ActionLink("新增", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InquiryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InquiryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactsPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.E_MAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalQty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product.ProductName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InquiryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InquiryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactsPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.E_MAIL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalQty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Product.ProductName)
        </td>
        <td>
            @Html.ActionLink("編輯", "Edit", new { id=item.InquiryID }) |
            @Html.ActionLink("檢視", "Details", new { id=item.InquiryID }) |
            @Html.ActionLink("刪除", "Delete", new { id=item.InquiryID })
        </td>
    </tr>
}

</table>
